<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
  <class name="com.example.tests.contacts.ContactData" table="addressbook">
    <id name="id" column="id" ></id>
    <property name="fname" column="firstname" />
    <property name="lname" column="lastname" />
    <property name="primaryAddr" column="address" />
	<property name="secondAddr" column="address2" />
	<property name="homePhone" column="home" />
	<property name="secondHomePhone" column="phone2" />
	<property name="mobilePhone" column="mobile" />
	<property name="workPhone" column="work" />
	<property name="firstEmail" column="email" />
	<property name="secondEmail" column="email2" />
	<property name="birthDay" column="bday" />
	<property name="birthMonth" column="bmonth" />
	<property name="birthYear" column="byear" />
  </class>
</hibernate-mapping>